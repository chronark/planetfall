NODE checks_by_team_by_month
SQL >

    %
    SELECT
    	checks.teamId,
      COUNT(*) as checks
    FROM
    	checks
     WHERE
      checks.time > fromUnixTimestamp({{Int64(start, description="Unix timestamp of billing period start", required=True)}}) 
      AND
       checks.time < fromUnixTimestamp({{Int64(end, description="Unix timestamp of billing period end", required=True)}})
     GROUP BY
    	checks.teamId


