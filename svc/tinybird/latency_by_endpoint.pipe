NODE daily_latencypercentiles_by_endpoint
SQL >

    select endpointId,date, quantiles(0.5, 0.95, 0.99)(latency) as percentiles
    from checks
    group by endpointId, timeSlot(checks.time) as date



NODE latency_for_endpoint
SQL >

    %
    SELECT * FROM daily_latencypercentiles_by_endpoint  WHERE endpointId={{String(endpointId, description="The endpointId to load latency percentiles", required=True)}}


