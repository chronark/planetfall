VERSION 1
NODE aggregate_globally
SQL >
    %
    SELECT
        COUNT(*) as count,
        min(latency) as min,
        max(latency) as max,
        quantile(0.5)(latency) as p50,
        quantile(0.95)(latency) as p95,
        quantile(0.99)(latency) as p99   
    FROM checks__v1
    WHERE  endpointId={{ String(endpointId, required=True)}} AND fromUnixTimestamp64Milli(time) >= now() - INTERVAL {{ Int32(days,7)}} DAY 
