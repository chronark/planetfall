VERSION 1
TOKEN "checks_last_24h" READ

NODE checks_last_24h
SQL >

    %
    SELECT time,
    regionId,
        quantile(0.99)(latency) as latency
    FROM checks
    WHERE 
      endpointId = {{String(endpointId, required=True)}}
     AND
      time > now() - interval 1 day
    GROUP BY toStartOfFiveMinute(fromUnixTimestamp64Milli(time)) as time, regionId
    ORDER BY time ASC




