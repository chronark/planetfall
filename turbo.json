{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "install": {
      "outputs": [
        "node_modules/**",
        "pnpm-lock.yaml"
      ]
    },
    "build": {
      "dependsOn": [
        "fmt",
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "fmt": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "test": {
      "cache": false
    }
  }
}